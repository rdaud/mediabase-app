ed(false)}>\n                       { isHovered && <Label>Mudar status</Label> } \n\n            <Status />\n        </ItemWrapper>\n    )\n\n}\n\nexport const Export = () => {\n    const [ isHovered, setIsHovered ] = useState(false)\n\n    return (\n        <ItemWrapper \n        onMouseEnter={() => setIsHovered(true)}\n        onMouseLeave={() => setIsHovered(false)}>\n                       { isHovered && <Label>Fazer o download</Label> } \n\n            <Download />\n        </ItemWrapper>\n    )\n\n}\n\nexport const ExportarCSV = () => {\n    const [ isHovered, setIsHovered ] = useState(false)\n\n    return (\n        <ItemWrapper \n        onMouseEnter={() => setIsHovered(true)}\n        onMouseLeave={() => setIsHovered(false)}>\n                       { isHovered && <Label>Exportar CSV</Label> } \n\n            <Document />\n        </ItemWrapper>\n    )\n\n}\n\nexport const Cancelar = ({...props}) => {\n    const [ isHovered, setIsHovered ] = useState(false)\n\n    return (\n        <ItemWrapper {...props}\n        onMouseEnter={() => setIsHovered(true)}\n        onMouseLeave={() => setIsHovered(false)}>\n                       { isHovered && <Label>Cancelar</Label> } \n\n            <Close />\n        </ItemWrapper>\n    )\n\n}","import React from 'react';\nimport { Wrapper,\n        Counter,\n        Remove,\n        AddCriativo,\n        ChangePrazo,\n        ChangeVeiculação,\n        ChangeStatus,\n        Export,\n        ExportarCSV,\n        Cancelar,\n        InnerWrapper\n    } from './styles'\n\n\nexport const Toolbar = ({counter, handleClickOnCancelar}) => {\n    return (\n        <Wrapper>\n      <InnerWrapper>\n        <Counter>\n            {counter > 1 ? `${counter} items selecionados` : `${counter} item selecionado`} \n            </Counter>\n        <Remove />\n        <AddCriativo />\n        <ChangePrazo />\n        <ChangeVeiculação />\n        <ChangeStatus />\n        <Export />\n        <ExportarCSV />\n      </InnerWrapper>\n      <I